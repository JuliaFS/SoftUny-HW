import java.util.regex.Matcher;
import java.util.regex.Pattern;


public class e {

	public static void main(String[] args) {
		String text = "<a id=\"sad\" href=\"linkref\">";
		Matcher p = Pattern.compile("<a.*href=\"(.*)\".*>", Pattern.CASE_INSENSITIVE).matcher(text);
		
		while(p.find()) {
			System.out.println(p.group(1));
		}
	}

}
